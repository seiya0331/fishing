<div class="continer">
  <div class="row">
    <div class="col">
      <h1>投稿フォーム</h1>
      <%= form_with model:@tweet, local: true do |f| %>
        <h4>画像</h4>
        <%= f.file_field :image, accept: "image/*" %>
        <p><%= f.label :text %><br>
           <%= f.text_area :text %>
        </p>
        <p><%= f.submit '投稿' %></p>
        <% end %>
    </div>
  </div>
</div>